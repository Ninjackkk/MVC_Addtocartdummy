@model SignIn

<span>@TempData["errorusername"]</span>

<span>@TempData["errorpassword"]</span>

<form method="post">

    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Email address</label>
        <input type="email" asp-for="Email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <span asp-validation-for="Email"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input type="password" asp-for="Password" class="form-control" id="exampleInputPassword1">
    </div>

    <button type="submit" class="btn btn-primary">SignIn</button>
</form>
